<%= link_to "Add a workspace", new_workspace_path %>

<% if @workspaces.any? %>
  <% @workspaces.each do |workspace| %>
    <h2><%= link_to workspace.name, workspace_path(workspace) %></h2>
    <%= link_to "Rate #{workspace.name}", new_workspace_rating_path(workspace) %>
  <% end %>
<% else %>
  <h1> No workspaces added </h1>
<% end %>
